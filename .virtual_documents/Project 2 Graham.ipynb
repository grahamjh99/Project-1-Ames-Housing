def make_dummies(df,test,column):
    df = pd.get_dummies(df, 
                        columns = [column],
                        prefix = column.strip(),
                        dtype = int)
    test = pd.get_dummies(test, 
                        columns=[column], 
                        prefix=column.strip(), 
                        dtype=int)

    return df, test


